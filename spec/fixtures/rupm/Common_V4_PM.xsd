                    <xsd:documentation>Charge</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Postal order">
                <xsd:annotation>
                    <xsd:documentation>Postal order</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
