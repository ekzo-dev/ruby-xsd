    <xsd:simpleType name="PatentDocumentIdentificationFormatCategoryType">
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="Original">
                <xsd:annotation>
                    <xsd:documentation>Original Number</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Standard">
                <xsd:annotation>
                    <xsd:documentation>Standard number</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="As stored">
                <xsd:annotation>
                    <xsd:documentation>As stored in exchanged databases</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="PatentDocumentKindCode" type="xsd:token">
        <xsd:annotation>
            <xsd:documentation>Kind code of patent document defined in WIPO Standard ST.16. Refers to ST.9 INID Code 13.</xsd:documentation>
        </xsd:annotation>
