            <xsd:enumeration value="Credit card">
                <xsd:annotation>
                    <xsd:documentation>Credit card</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Debit card">
                <xsd:annotation>
                    <xsd:documentation>Debit card</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Card">
