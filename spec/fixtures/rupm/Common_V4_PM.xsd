            <xsd:documentation>Identifier for party</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PartyIdentifierBag" type="com:PartyIdentifierBagType">
        <xsd:annotation>
            <xsd:documentation>Collection of identifiers identifying a party</xsd:documentation>
        </xsd:annotation>
