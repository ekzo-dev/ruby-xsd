                    <xsd:documentation>Former name</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Common use">
                <xsd:annotation>
                    <xsd:documentation>Common use</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Publishing name">
