                    <xsd:documentation>Publishing name</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Unofficial name">
                <xsd:annotation>
                    <xsd:documentation>Unofficial name</xsd:documentation>
                </xsd:annotation>
