                    <xsd:documentation>Done</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Attached">
                <xsd:annotation>
                    <xsd:documentation>Attached</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="To follow">
                <xsd:annotation>
                    <xsd:documentation>To follow</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Bank transfer to follow">
                <xsd:annotation>
                    <xsd:documentation>Bank transfer to follow</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Undefined">
                <xsd:annotation>
                    <xsd:documentation>Undefined</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
