            <xsd:enumeration value="In month">
                <xsd:annotation>
                    <xsd:documentation>In month</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="In week">
                <xsd:annotation>
                    <xsd:documentation>In week</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="In day">
                <xsd:annotation>
                    <xsd:documentation>In day</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
