    <xsd:element name="PayeeName" type="com:NameType">
        <xsd:annotation>
            <xsd:documentation>Name details of the payee of the reimbursement</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PayerName" type="com:NameType">
        <xsd:annotation>
            <xsd:documentation>Name details of the payer</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Payment" type="com:PaymentType">
        <xsd:annotation>
            <xsd:documentation>Data regarding a request for payment</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
