            <xsd:enumeration value="R00">
                <xsd:annotation>
                    <xsd:documentation>
                        No mapped category R detailed event: the national/regional event cannot be mapped to a detailed event in the category R.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="S00">
                <xsd:annotation>
                    <xsd:documentation>
                        No mapped category S detailed event: the national/regional event cannot be mapped to a detailed event in the category S.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="T00">
                <xsd:annotation>
                    <xsd:documentation>
                        No mapped category T detailed event: the national/regional event cannot be mapped to a detailed event in the category T.
                    </xsd:documentation>
                </xsd:annotation>
