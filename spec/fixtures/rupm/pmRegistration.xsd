    <xs:element name="RequestWrapper" type="tns:RequestWrapperType" />
    <xs:element name="ResponseWrapper" type="tns:ResponseWrapperType" />
    <xs:complexType name="RequestWrapperType">
        <xs:sequence>
            <xs:element name="OrderId" type="xs:long">
                <xs:annotation>
                    <xs:documentation>Идентификатор заявления</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ServiceCode" type="tns:string-20" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код услуги</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TargetCode" type="tns:string-20" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код цели</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OrderTimestamp" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>Дата и время формирования запроса на ЕПГУ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PersonProfile" type="tns:PersonProfileType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Данные из профиля заявителя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="CancelRequest" type="tns:CancelRequestType">
                    <xs:annotation>
                        <xs:documentation>Запрос отмены заявления</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Application" type="tns:ApplicationType">
                    <xs:annotation>
                        <xs:documentation>Данные заявления</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="transactionKind">
            <xs:annotation>
                <xs:documentation>Тип записи о транзакции</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Registration">
                        <xs:annotation>
                            <xs:documentation>Первичная регистрация</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Change">
                        <xs:annotation>
                            <xs:documentation>Изменение данных об авторе, правообладателе, адресе и др.</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Continuation">
                        <xs:annotation>
                            <xs:documentation>Возобновление действия</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Discontinuation">
                        <xs:annotation>
                            <xs:documentation>Прекращение действия</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="LegalStatus">
                        <xs:annotation>
                            <xs:documentation>Изменение статуса</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Licence">
                        <xs:annotation>
                            <xs:documentation>Вопросы лицензирования</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Correction">
                        <xs:annotation>
                            <xs:documentation>Коррекция технических ошибок</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Other">
                        <xs:annotation>
                            <xs:documentation>Прочие изменения</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="baseRequestNumber" type="xs:string">
            <xs:annotation>
                <xs:documentation>Номер первоначального заявления</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="DocumentsType">
        <xs:sequence>
            <xs:element name="DocCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код документа (служебное наименование/код/мнемоника)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FileName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Имя файла документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IsArchive" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Передача в архиве</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="URI" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Путь к файлу в архиве</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FileType" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>MIME-тип контента</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Description" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Описание</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="String-3000">
        <xs:restriction base="xs:string">
            <xs:maxLength value="3000" />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ResponseWrapperType">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Корневой элемент ответа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="OrderId" type="xs:long">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Идентификатор заявления</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="ChangeOrderInfo" type="tns:ChangeOrderInfoType">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Изменение статуса</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="CancelResponse" type="tns:CancelResponseType">
                    <xs:annotation>
                        <xs:documentation>Ответ на запрос отмены заявления</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ChangeOrderInfoType">
        <xs:annotation>
            <xs:documentation>Информация для изменения статуса заявления для направления в ЛК ЕПГУ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="StatusCode" type="tns:StatusCodeType">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Код статуса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Comment" type="tns:string-2000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Комментарий ведомства к статусу</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CancelAllowed" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Возможность отменить заявление</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="StatusCodeType">
        <xs:annotation>
            <xs:documentation>Статус заявления</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="OrgCode" type="tns:string-50">
                <xs:annotation>
                    <xs:documentation>Код статуса в ведомстве</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TechCode" type="xs:byte">
                <xs:annotation>
                    <xs:documentation>Код статуса на ЕПГУ</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:simpleType name="string-2000">
        <xs:restriction base="xs:string">
            <xs:maxLength value="2000" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="string-20">
        <xs:restriction base="xs:string">
            <xs:maxLength value="20" />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="PersonProfileType">
        <xs:annotation>
            <xs:documentation>Профиль заявителя</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="NameCompany" type="xs:string" minOccurs="0">
