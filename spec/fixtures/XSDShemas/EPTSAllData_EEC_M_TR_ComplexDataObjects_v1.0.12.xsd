	<xs:import namespace="urn://x-artefacts-epts-ru/EPTSAllData_EEC_M_ComplexDataObjects/0.4.14" schemaLocation="EPTSAllData_EEC_M_ComplexDataObjects_v0.4.14.xsd"/>
