            <xs:annotation>
               <xs:documentation source="ddex:Definition">The StartDate of the membership. This is a string with the syntax YYYY[-MM[-DD]].</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="EndDate" minOccurs="0" type="mlc:ddex_IsoDate">
