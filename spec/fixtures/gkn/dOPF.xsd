<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:simpleType name="dOPF">
		<xs:annotation>
			<xs:documentation>Классификатор "Организационно правовые формы юридических лиц"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Классификационный код">
				<xs:annotation>
					<xs:documentation>Значение</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="009001000000">
				<xs:annotation>
					<xs:documentation>Юридические лица, являющиеся коммерческими организациями</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="009001001000">
				<xs:annotation>
					<xs:documentation>Полное товарищество</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="009001002000">
				<xs:annotation>
					<xs:documentation>Товарищество на вере (коммандитное товарищество)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="009001003000">
				<xs:annotation>
					<xs:documentation>Общество с ограниченной ответственностью</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="009001004000">
				<xs:annotation>
					<xs:documentation>Общество с дополнительной ответственностью</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="009001005000">
