					<xs:documentation>Открытое акционерное общество</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="009001006000">
				<xs:annotation>
					<xs:documentation>Закрытое акционерное общество</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="009001007000">
